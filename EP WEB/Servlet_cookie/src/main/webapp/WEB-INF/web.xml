<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Servlet_cookie</display-name>
  <servlet>
        <servlet-name>CreateCookieServlet</servlet-name>
        <servlet-class>cookie.CreateCookieServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateCookieServlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DisplayCookiesServlet</servlet-name>
        <servlet-class>cookie.DisplayCookiesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DisplayCookiesServlet</servlet-name>
        <url-pattern>/DisplayCookie</url-pattern>
    </servlet-mapping>
</web-app>